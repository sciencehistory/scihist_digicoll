<div class="mx-1 my-2"><strong>NOTES</strong></div>
<div class="footnote-list mx-1 mb-5">
    <% footnote_array.each_with_index.map do |footnote_text, i| %>
        <div class="footnote-page-bottom-container">
          <a name="footnote<%= i + 1 %>" id="footnote<%= i + 1 %>"></a>
          <a data-role="footnote-page-bottom" data-footnote-index="<%= i + 1 %>" href="#" >
          <%= i + 1 %>.</a> <%= footnote_text%>
        </div>
    <% end %>
</div>