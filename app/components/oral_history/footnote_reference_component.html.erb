    title="<%= footnote_text %>"
    id="footnote-reference-<%= number %>"
    aria-label="footnote <%= number %>"
    aria-describedby="footnote-text-<%=number%>"
    data-role="ohms-navbar-aware-internal-link"
    href="#footnote-<%= number%>">
  [<%= number %>]
</a>

